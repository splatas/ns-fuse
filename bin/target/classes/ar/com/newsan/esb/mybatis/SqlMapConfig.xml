<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="useGeneratedKeys" value="false"/>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
    <typeAliases>
        <typeAlias type="ar.com.newsan.esb.model.Item" alias="Item" />
        <typeAlias type="ar.com.newsan.esb.model.Customer" alias="Customer" />
        <typeAlias type="ar.com.newsan.esb.model.ViewOrder" alias="ViewOrder" />
        <typeAlias type="ar.com.newsan.esb.model.ViewOrderLine" alias="ViewOrderLine" />
        <typeAlias type="ar.com.newsan.esb.model.ProductComponent" alias="ProductComponent" />
        <typeAlias type="ar.com.newsan.esb.model.ProductPrice" alias="ProductPrice" />
        <typeAlias type="ar.com.newsan.esb.model.ProductStock" alias="ProductStock" />
        <typeAlias type="ar.com.newsan.esb.model.Supplier" alias="Supplier" />
        <typeAlias type="ar.com.newsan.model.ApiReceptionTransaction" alias="ReceptionTransaction" />
        <typeAlias type="ar.com.newsan.esb.model.POReception" alias="POReception" />
    </typeAliases>
</configuration>